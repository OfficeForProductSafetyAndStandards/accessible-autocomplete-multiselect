{
  "name": "@officeforproductsafetyandstandards/accessible-autocomplete-multiselect",
  "version": "3.0.5",
  "main": "dist/accessible-autocomplete.min.js",
  "style": "dist/accessible-autocomplete.min.css",
  "description": "An autocomplete component, built to be accessible.",
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeForProductSafetyAndStandards/accessible-autocomplete-multiselect.git"
  },
  "author": "Government Digital Service (https://www.gov.uk/government/organisations/government-digital-service) and Office for Product Safety & Standards (https://www.gov.uk/government/organisations/office-for-product-safety-and-standards)",
  "license": "MIT",
  "keywords": [
    "a11y",
    "accessibility",
    "autocomplete",
    "component",
    "plugin",
    "typeahead",
    "widget"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress",
    "dev": "cross-env NODE_ENV=development webpack serve",
    "karma:dev": "cross-env NODE_ENV=test karma start test/karma.config.js",
    "karma": "npm run karma:dev -- --single-run",
    "prepare": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module",
    "preversion": "npm test",
    "standard": "standard",
    "pretest": "npm run build",
    "test": "run-p standard karma wdio",
    "version": "git add -A dist",
    "wdio": "cross-env NODE_ENV=test wdio test/wdio.config.js"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/eslint-parser": "^7.28.4",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@babel/register": "^7.28.3",
    "@wdio/cli": "^9.20.0",
    "@wdio/globals": "^9.17.0",
    "@wdio/local-runner": "^9.20.0",
    "@wdio/mocha-framework": "^9.20.0",
    "@wdio/sauce-service": "^9.20.0",
    "@wdio/spec-reporter": "^9.20.0",
    "@wdio/static-server-service": "^9.20.0",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "babel-plugin-istanbul": "^7.0.1",
    "chai": "^6.2.0",
    "chalk": "^5.6.2",
    "core-js": "^3.45.1",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "cssnano": "^7.1.1",
    "cssnano-preset-default": "^7.0.9",
    "devtools": "^8.39.1",
    "dotenv": "^17.2.3",
    "husky": "^9.1.7",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.4.0",
    "karma-webpack": "^5.0.1",
    "mini-css-extract-plugin": "^2.9.4",
    "mocha": "^11.7.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.2.0",
    "preact": "^8.5.3",
    "puppeteer": "^24.25.0",
    "source-map-loader": "^5.0.0",
    "standard": "^17.1.2",
    "terser-webpack-plugin": "^5.3.9",
    "webdriverio": "^9.20.0",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "peerDependencies": {
    "preact": "^8.0.0"
  },
  "peerDependenciesMeta": {
    "preact": {
      "optional": true
    }
  },
  "standard": {
    "ignore": [
      "dist"
    ],
    "parser": "@babel/eslint-parser"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "overrides": {
    "tar-fs": "^3.0.9",
    "ws": "^8.17.1"
  }
}
